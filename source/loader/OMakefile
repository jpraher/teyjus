LD_C[] =
    loader
    strings
    file
    kind
    const
    hashtab
    code
    bvrtab
    implgoal
    searchtab
    importtab
    tyskel

LIB_FILES[] =
    $(SIM_C)
    $(SYS_C)
    $(TBL_C)
    $(LD_C)

LIB = libloader

StaticCLibrary($(LIB), $(LIB_FILES))

MAIN = loadit

MAIN_FILES[] =
    main

LIBS = $(LIB)

CFLAGS += -lm

CProgram($(MAIN), $(MAIN_FILES))
