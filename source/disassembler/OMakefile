CMP_SRCS[] =
    $(CMP)/bytecode
    $(CMP)/absyn
    $(CMP)/table
    $(CMP)/symbol
    $(CMP)/option
    $(CMP)/errormsg
    $(CMP)/pervasive
    $(CMP)/pervasiveutils
    $(CMP)/instr

DIS_SRCS[] =
    context
    disassembly
    label
    main

.DEFAULT: $(OCamlProgram tjdis, $(CMP_SRCS) $(DIS_SRCS))
