CFLAGS += -g -O0 -Wall

LK_C[] =
    CallResolution
    bvrtab
    const
    hashtab
    importtab
    rename
    tyskel
    code
    file
    implgoal
    kind
    module
    stringspace
    tree
    vector
    VectorRW

LIB_FILES[] =
    $(SIM_C)
    $(SYS_C)
    $(TBL_C)
    $(LK_C)

# Linker library

LIB = liblinker

StaticCLibrary($(LIB), $(LIB_FILES))

# Linker program

MAIN = linker

MAIN_FILES[] =
    main

LIBS = $(LIB)


CProgram($(MAIN), $(MAIN_FILES))
