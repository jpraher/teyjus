# The OMake build only supports cygwin on win32
if $(mem $(OSTYPE), Win32 Cygwin)
    OSTYPE = Cygwin
    GCC_FOUND = true
    GXX_FOUND = true
    export

open build/C
open build/OCaml

Deriving(module) =
    section
        OCAMLOPTFLAGS += -pp deriving
        OCAMLDEPFLAGS += -pp deriving
        $(module).cmi:
        $(module).cmo:
        $(module).o:
        $(module).cmx:


.SUBDIRS: .
